    <details class="specifications" id="statistics">
        <div class="row fond">
        {% set nbr = 0 %}
        {% for fluxpost in fluxPosts %}
             {{  fluxpost.post.description }}
            <br/><small>{{  fluxpost.createdat|date('d/m/y') }} </small>
            {% set nbr = nbr +1 %}
            {% for p in fluxpost.post.photos %}
                {% if p.imageName != "aujourdhui.png"  %}
                    {{ include('carnet/display_photo.html.twig', { 'p': p }) }}
                {% endif %}   
            {% endfor %}
            <br/>
      {% endfor %}

        </div>
        <summary class="row cadre_blanc">
            <span class="deroulfloatl">&nbsp;<img src="{{ asset('images/a_propos.png') }}" alt="Image" width="30" height="30">&nbsp;&nbsp;Notes ({{ nbr }})</span>
            <span class="deroulfloatr"><span class="chevron glyphicon glyphicon-menu-right"> </span>&nbsp;</span>
        </summary>
    </details>
